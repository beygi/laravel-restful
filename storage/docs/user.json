{
    "basePath": "/api",
    "swaggerVersion": "1.2",
    "apiVersion": "1.0",
    "resourcePath": "/user",
    "apis": [
        {
            "path": "/user",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get user",
                    "nickname": "getAuthenticatedUser"
                }
            ]
        },
        {
            "path": "/user/authenticate",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Get token",
                    "nickname": "authenticate",
                    "parameters": [
                        {
                            "paramType": "form",
                            "name": "email",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "user email"
                        },
                        {
                            "paramType": "form",
                            "name": "password",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "user password"
                        }
                    ]
                }
            ]
        }
    ],
    "produces": [
        "application/json"
    ]
}